    "eject": "cd ./packages/client && npm eject",
    "serve": "cd ./packages/client && npm serve"
  },
  "devDependencies": {
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "5.2.1"
  }
}
